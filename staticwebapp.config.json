{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/writings/*",
      "serve": "/writings/index.html"
    },
    {
      "route": "/profile/*",
      "serve": "/profile/index.html"
    },
    {
      "route": "/search/*",
      "serve": "/search/index.html"
    },
    {
      "route": "/static/*",
      "serve": "/static/"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*"]
  }
}